{
    "name": "pantheon-upstreams/drupal-composer-managed",
    "description": "Install Drupal 9 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "path",
            "url": "upstream-configuration"
        }
    ],
    "require": {
        "pantheon-upstreams/upstream-configuration": "dev-main",
        "composer/installers": "^1.9",
        "drupal/core-composer-scaffold": "10.5.1",
        "drupal/core-recommended": "10.5.1",
        "pantheon-systems/drupal-integrations": "^10",
        "cweagans/composer-patches": "^1.7",
        "drush/drush": "^12",
        "drupal/pathauto": "1.x-dev@dev",
        "drupal/imce": "^3.0",
        "drupal/devel": "^5.4",
        "drupal/admin_toolbar": "^3.6",
        "drupal/ckeditor_link_styles": "^1.0",
        "drupal/twig_tweak": "^3.4",
        "drupal/token_filter": "^2.1",
        "drupal/sitemap": "2.x-dev@dev",
        "drupal/redirect": "^1.9",
        "drupal/metatag": "^2.1",
        "drupal/sendgrid_integration": "2.x-dev@dev",
        "drupal/ajax_loader": "2.x-dev@dev",
        "drupal/email_tfa": "^2.0",
        "drupal/view_password": "6.0.x-dev@dev",
        "drupal/smtp": "^1.2",
        "kint-php/kint": "^5.1",
        "drupal/link_attributes": "^2.0",
        "drupal/login_security": "2.x-dev@dev",
        "drupal/salesforce": "5.1.2",
        "drupal/key": "^1.20",
        "drupal/address": "^2.0",
        "drupal/dynamic_entity_reference": "^3.2",
        "drupal/typed_data": "^2.1",
        "drupal/webform": "^6.3@beta",
        "drupal/restui": "^1.21",
        "drupal/redirect_after_logout": "^2.0",
        "drupal/flood_control": "^3.0",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/paragraphs": "^1.17",
        "drupal/estimated_read_time": "^1.0",
        "drupal/quick_node_clone": "^1.18",
        "drupal/smart_trim": "^2.1",
        "drupal/devel_kint_extras": "^1.1",
        "drupal/responsive_favicons": "^3.0",
        "drupal/gcsfs": "^1.0@beta",
        "drupal/entity": "^1.4",
        "drupal/password_policy": "^4.0",
        "drupal/structure_sync": "^2.0",
        "drupal/field_group": "^4.0",
        "drupal/view_custom_table": "^2.0",
        "drupal/better_exposed_filters": "^7.0",
        "drupal/ultimate_cron": "^2.0@beta",
        "drupal/purge_users": "^4.0",
        "drupal/rest_api_authentication": "^2.0",
        "drupal/salesforce_oauth_client_credentials": "^1.0",
        "drupal/reroute_email": "^2.3@RC",
        "drupal/editor_advanced_link": "^2.3",
        "drupal/persistent_login": "^2.1",
        "drupal/ajax_command_page_reload": "^2.0",
        "drupal/csv_importer": "^2.0",
        "drupal/ga4_google_analytics": "^1.1",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/config_ignore": "^3.3",
        "drupal/s3fs": "^3.8",
        "drupal/xmlsitemap": "^2.0",
        "drupal/queue_ui": "^3.1",
        "drupal/webprofiler": "^10.1",
        "drupal/menu_per_role": "1.x-dev@dev",
        "drupal/google_tag": "^2.0",
        "google/cloud-bigquery": "^1.30",
        "drupal/core-project-message": "10.5.1",
        "drupal/datalayer": "^2.1",
        "drupal/jquery_ui_datepicker": "^2.1",
        "drupal/jquery_ui_slider": "^2.1",
        "drupal/jquery_ui_touch_punch": "^1.1",
        "drupal/devel_php": "^1.7",
        "ext-zlib": "*",
        "drupal/tracer": "^1.0",
        "drupal/mailgun": "2.0.0",
        "drupal/recaptcha_v3": "^2.0",
        "drupal/simple_oauth": "^6.0"
    },
    "require-dev": {
        "drupal/core-dev": "10.4.5",
        "squizlabs/php_codesniffer": "^3.10"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "allowed-packages": ["pantheon-systems/drupal-integrations"],
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/pantheon.upstream.yml": false,
                "[project-root]/.gitattributes": false,
                "[web-root]/robots.txt": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "web/private/scripts/quicksilver/{$name}/": ["type:quicksilver-script"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "enable-patching": true,
        "patches": {
            "drupal/core": {
                "TypeError: htmlspecialchars(): Argument #1 ($string) must be of type string, array given in htmlspecialchars()": "https://www.drupal.org/files/issues/2023-08-07/TypeError_htmlspecialchars_ArgumentNustBeTypeString_ArrayGiven-3352384-43.patch",
                "Email Address should be a required field in the 'Add User' form": "https://www.drupal.org/files/issues/2023-12-03/3303256-15.patch"
            },
            "drupal/email_tfa": {
                "Fix notices for user replacement tokens": "./patches/email_tfa-user_tokens_replacement.patch"
            },
            "drupal/twig_tweak": {
                "Add user context for menu cache": "./patches/twig_tweak-add_user_context_for_menu_cache.patch"
            },
            "drupal/salesforce": {
                "Add auth provider overrides": "./patches/salesforce_overrides.patch"
            }
        }
    },
    "autoload": {
        "classmap": ["upstream-configuration/scripts/ComposerScripts.php"]
    },
    "scripts": {
        "pre-update-cmd": ["DrupalComposerManaged\\ComposerScripts::preUpdate"],
        "post-update-cmd": ["DrupalComposerManaged\\ComposerScripts::postUpdate"]
    },
    "config": {
        "process-timeout": 0,
        "preferred-install": "dist",
        "sort-packages": false,
        "platform": {
            "php": "8.2.0"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true,
            "drupal/core-project-message": true,
            "tbachert/spi": true
        }
    }
}
